function createMenu() {

    setTimeout(function() {


        // Gets the course type from the data-course attribute on html page
        let courseType = document.querySelector("#page-info").dataset.course

        // Gets the page type from the data-page attribute on html page
        let pageType = document.querySelector("#page-info").dataset.page



        const currentMenu = activeMenus.find(e => e.name === courseType)
        console.log(currentMenu)

        var ul = document.createElement('ul');

        try {

            currentMenu.mainMenu.forEach(e => {
                const li = document.createElement('li')
                const text = document.createElement('a')
                text.innerHTML = e.name
                text.setAttribute("href", e.url);

                switch (e.name) {
                    case pageType:
                        text.setAttribute("class", "Button Button--info Button--active");
                        break;
                    case 'Tickets':
                        text.setAttribute("class", "Button Button--info Button--warning");
                        break;

                    default:
                        text.setAttribute("class", "Button Button--info");
                        break;
                }


                li.appendChild(text)
                ul.appendChild(li)
            })

            document.querySelector("#global-nav").appendChild(ul)



            const dropdown_container = document.createElement('div')
            dropdown_container.setAttribute("class", "al-dropdown__container");

            const dropdown_container_text = document.createElement('a')
            dropdown_container_text.setAttribute("class", "al-trigger Button");
            dropdown_container_text.setAttribute("role", "button");
            dropdown_container_text.setAttribute("href", "#");
            dropdown_container_text.setAttribute("aria-haspopup", "true");
            dropdown_container_text.setAttribute("tabindex", "0");
            dropdown_container_text.innerHTML = `Quick Links <i class="icon-mini-arrow-down"></i>`

            dropdown_container.appendChild(dropdown_container_text)

            const dropdown_ul = document.createElement('ul')
            dropdown_ul.setAttribute("class", "al-options ui-menu ui-widget ui-widget-content ui-corner-all ui-popup ui-kyle-menu use-css-transitions-for-show-hide");
            dropdown_ul.setAttribute("aria-expanded", "false");
            dropdown_ul.setAttribute("aria-hidden", "true");
            dropdown_ul.setAttribute("tabindex", "0");
            dropdown_ul.setAttribute("role", "menu");
            dropdown_ul.setAttribute("id", "ui-id-4");
            dropdown_ul.setAttribute("style", "display: none; top: 39.8px; left: 8.34998px;");
            dropdown_ul.setAttribute("aria-activedescendant", "ui-id-1");

            currentMenu.otherDropdownButton.forEach((e, i) => {
                const dropdown_li = document.createElement('li')
                dropdown_li.setAttribute("class", "ui-menu-item");
                dropdown_li.setAttribute("role", "presentation");

                const dropdown_li_a = document.createElement('a')
                dropdown_li_a.innerHTML = e.name
                dropdown_li_a.setAttribute("title", e.name);
                dropdown_li_a.setAttribute("href", e.link);
                dropdown_li_a.setAttribute("data-api-endpoint", e.endpoint);
                dropdown_li_a.setAttribute("data-api-returntype", "Page");
                dropdown_li_a.setAttribute("class", "icon-edit ui-corner-all");
                dropdown_li_a.setAttribute("tabindex", "-1");
                dropdown_li_a.setAttribute("role", "menuitem");
                dropdown_li_a.setAttribute("id", "ui-id-" + i++);
                dropdown_li.appendChild(dropdown_li_a)
                dropdown_ul.appendChild(dropdown_li)
            })


            dropdown_container.appendChild(dropdown_ul)
            const li = document.createElement('li')
            li.appendChild(dropdown_container)
            ul.appendChild(li)




        } catch (error) {
            console.error(error)
        }

    }, 2000);


}

